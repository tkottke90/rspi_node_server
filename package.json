{
  "name": "rspi-server-1-0",
  "description": "RaspberryPi Server using Feathers/Express",
  "version": "0.0.0",
  "homepage": "",
  "main": "src",
  "keywords": [
    "feathers",
    "raspberry-pi",
    "johnny-five"
  ],
  "author": {
    "name": "Thomas Kottke",
    "email": "t.kottke90@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^10.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run eslint && NODE_ENV= npm run mocha",
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "dev": "nodemon src/",
    "start": "node src/",
    "mocha": "mocha test/ --recursive --exit",
    "build": "node-sass ./sass -o ./public",
    "serve": "npm run build && http-server public/",
    "deploy": "npm run build && rsync -a -e ssh . pi@192.168.1.26:/home/pi/app",
    "login": "ssh pi@192.168.1.26"
  },
  "dependencies": {
    "@feathersjs/configuration": "^2.0.6",
    "@feathersjs/errors": "^3.3.6",
    "@feathersjs/express": "^1.3.1",
    "@feathersjs/feathers": "^3.3.1",
    "@feathersjs/socketio": "^3.2.9",
    "@types/socket.io": "^2.1.2",
    "chalk": "^2.4.2",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "d3": "^5.9.1",
    "helmet": "^3.15.0",
    "lit-html": "^1.0.0",
    "node-dht-sensor": "^0.0.34",
    "serve-favicon": "^2.5.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "^5.12.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  }
}
